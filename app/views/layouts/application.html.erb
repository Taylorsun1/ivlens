
<%= render 'layouts/header' %>
<meta name="viewport" content="width=device-width, initial-scale=1.0">

<% flash.each do |name, msg| %>
     <%= content_tag(:div, msg, class: "alert alert-#{name}") %>
<% end %>


<head>
